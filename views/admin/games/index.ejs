<h1>Games</h1>
<a href="/admin/games/new" class="btn btn-primary">
    Add game
</a>
<a href="/admin/score/new" class="btn btn-primary">
    Add highscore
</a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Genre</th>
            <th>Release date</th>
        </tr>
    </thead>
    <tbody>
        <% games.forEach(game=> { %>
            <tr>
                <td><%= game.id %></td>
                <td><%= game.title %></td>
                <td><%= game.genre %></td>
                <td><%= game.release_date %></td>
            </tr>
        <% }) %>
    </tbody>
</table>