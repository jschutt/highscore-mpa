<section class="row mb-4">
  <img src="<%= game.image_url%>" class="d-block col-5" alt="" />
  <div class="col-7">
    <h1 class="mb-3"><%= game.title %></h1>
    <div class="mb-3"><%= game.release_date %>, <%= game.genre %></div>
    <div class="mb-3"><%= game.description%></div>
  </div>
  <h2 class="text-center my-3">Highscores</h2>
  <div class="row mx-auto">
    <% users.forEach(user=> { %>
    <div class="user-card mb-2 d-flex justify-content-between">
      <p><%= user.player%>, <%= user.highscore_date%></p>
      <p><%= user.highscore%></p>
    </div>
    <% }) %>
    </div>
</section>
